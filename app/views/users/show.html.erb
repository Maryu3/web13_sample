
<h1><%= @user.name %>のページ</h1>

<%= link_to "#{@user.name}のお気に入り一覧",likes_user_path(@user) %>

<h1>自分の投稿一覧</h1>

<% @user.posts.each do |post|%>
	<div>
		<h1>
			<%= link_to "タイトル：#{post.title}" ,post_path(post.id) %>
		</h1>
		<span>
			<%= link_to "削除", destroy_post_path(post), method: :delete %>
		</span>
	</div>

<% end%>